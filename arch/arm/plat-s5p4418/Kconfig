# Select Board types
comment "Board features"

choice
    prompt "Platform board"
    default PLAT_S5P4418_iA1

	config PLAT_S5P4418_SVT
		bool "SVT: Linux svt board"
		help
   	 		Support for the svt board.

	config PLAT_S5P4418_MSD8000B
		bool "AVN: Android msd8000b AVN board"
		help
   	 		Support for the msd8000b avn board.

	config PLAT_S5P4418_AVN_REF
		bool "AVN: Android AVN reference board"
		help
   	 		Support for the AVN reference board.

	config PLAT_S5P4418_NAVI_REF
		bool "NAVI: Android NAVI reference board"
		help
   	 		Support for the NAVI reference board.

	config PLAT_S5P4418_iA1
		bool "iA1: Android iA1 board"
		help
   	 		Support for the iA1 board.

	config PLAT_S5P4418_iA2
		bool "iA2: Android iA2 board"
		help
   	 		Support for the iA2 board.

endchoice

